<% if(success_msg!= ""){%>
<div class="alert alert-success alert-dismissible fade show w-50 alert-center" role="alert">
  <strong><%=success_msg%></strong>
  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<%}%>


<% if(error_msg!= ""){%>

  <div class="alert alert-warning alert-dismissible fade show w-50 alert-center" role="alert">
    <strong><%=error_msg%></strong>
    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
      <span aria-hidden="true">&times;</span>
    </button>
  </div>


<%}%>
